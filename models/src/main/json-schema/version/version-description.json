{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "javaName": "VersionDescription",
  "properties": {
    "commitId": {
      "type": "string",
      "description": "The Id linked to this version.",
      "id": "commitId"
    },
    "version": {
      "type": "string",
      "description": "The string of the version.",
      "id": "version"
    },
    "createdAt": {
      "type": "string",
      "description": "When the version was created.",
      "id": "createdAt"
    },
    "versionParts": {
      "type": "array",
      "description": "An array of the version, split on <dot>.",
      "items": {
        "type": "string"
      },
      "id": "versionParts"
    },
    "postfix": {
      "type": "string"
    }
  },
  "id": "/",
  "required": [
    "commitId",
    "version",
    "createdAt",
    "versionParts"
  ]
}